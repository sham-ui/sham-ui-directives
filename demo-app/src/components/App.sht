<main>
    <pre>
        Directives for sham-ui-templates
    </pre>
    <p>disabled = {{disabled}}</p>
    <p>
        <input :disabled={{disabled}} value="disabled"/>
    </p>
    <p>
        <input :disabled={{!disabled}} value="!disabled"/>
    </p>
    <p>
        <button :onclick={{click.bind(this)}}>
            Click me
        </button>
    </p>
</main>
